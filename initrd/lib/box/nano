patches=""

export CROSS_COMPILE="$TGT-"

export NCURSESW_CFLAGS="-I${out}/usr/include"
export NCURSESW_LIBS="-L${out}/usr/lib -lncursesw"

config_options="--prefix=/usr --libdir=/usr/lib --enable-help --enable-utf8"
config_options="$config_options --enable-linenumbers --enable-multibuffer --enable-nanorc"
config_options="$config_options --disable-browser --disable-color --disable-comment --disable-extra"
config_options="$config_options --disable-histories --disable-justify --disable-libmagic"
config_options="$config_options --disable-mouse --disable-operatingdir --disable-speller"
config_options="$config_options --disable-tabcomp --disable-wordcomp --disable-rpath"
config_options="$config_options --without-libiconv-prefix --without-libintl-prefix"

